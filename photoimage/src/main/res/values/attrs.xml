<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--坐标轴线条粗细-->
    <attr name="coordinatesLineWidth" format="dimension"></attr>
    <!--坐标轴字体大小-->
    <attr name="coordinatesTextSize" format="dimension"></attr>
    <!--坐标轴字体颜色-->
    <attr name="coordinatesTextColor" format="color"></attr>
    <!--线条颜色-->
    <attr name="lineColor" format="color"></attr>
    <!--线条粗细-->
    <attr name="lineWidth" format="dimension"></attr>
    <!--小圆点半径-->
    <attr name="averageCircleradius" format="dimension"></attr>
    <!--数据类型-->
    <attr name="tableType" format="string"></attr>
    <!--大圆点颜色-->
    <attr name="maxcircleColor" format="color"></attr>
    <!--小圆点颜色-->
    <attr name="mincircleColor" format="color"></attr>
    <!--背景色-->
    <attr name="bgColor" format="color"></attr>

    <declare-styleable name="LineChart">
        <attr name="coordinatesLineWidth"/>
        <attr name="coordinatesTextSize"/>
        <attr name="coordinatesTextColor"/>
        <attr name="lineColor"/>
        <attr name="lineWidth"/>
        <attr name="averageCircleradius"/>
        <attr name="tableType"/>
        <attr name="maxcircleColor"/>
        <attr name="mincircleColor"/>
        <attr name="bgColor"/>
    </declare-styleable>


</resources>